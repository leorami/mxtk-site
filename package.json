{
  "name": "mxtk-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:proxy": "NEXT_PUBLIC_BASE_PATH=/mxtk next dev",
    "build": "node scripts/guardrails/check-basepath.js && next build",
    "start": "node scripts/guardrails/check-basepath.js && next start",
    "docker:dev": "docker compose up --build web",
    "docker:down": "docker compose down",
    "docker:prod:build": "docker build -t mxtk-site:prod --target runner .",
    "docker:prod:run": "docker run --rm -p 2001:2000 mxtk-site:prod",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:navigation": "node tools/test/nav-regression.mjs",
    "test:nav:localhost": "BASE_URL=http://localhost:2000 node tools/test/nav-regression.mjs",
    "test:nav:ngrok": "BASE_URL=https://ramileo.ngrok.app/mxtk node tools/test/nav-regression.mjs",
    "test:regression": "npm run test:navigation",
    "test:full": "ENABLE_EXTERNAL_TESTS=1 node tools/debug/navigation-regression.js",
    "test:int": "BASE_URL=http://localhost:2000 tools/test_runner.sh home",
    "make:org-logos": "ts-node scripts/make-org-placeholders.ts || node -e \"require('esbuild-register'); require('./scripts/make-org-placeholders.ts')\" || node -e \"import('./scripts/make-org-placeholders.ts')\"",
    "ai:ingest": "tsx scripts/ai.ingest.ts"
  },
  "dependencies": {
    "@types/recharts": "^1.8.29",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "mammoth": "^1.10.0",
    "next": "^15.0.0",
    "pdfjs-dist": "^4.6.82",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^9.1.0",
    "react-scroll-parallax": "^3.4.5",
    "recharts": "^3.1.2",
    "remark-gfm": "^4.0.1",
    "use-debounce": "^10.0.6",
    "viem": "^2.7.9",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "22.17.2",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "18.3.23",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "puppeteer": "^24.17.1",
    "sass": "^1.77.0",
    "tailwindcss": "^3.4.7",
    "tsx": "^4.20.5",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
